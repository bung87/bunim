{
  "name": "bunim",
  "module": "index.ts",
  "type": "module",
  "bin": {
    "bunim": "./index.ts"
  },
  "scripts": {
    "link": "bun run scripts/link.ts",
    "unlink": "bun run scripts/link.ts unlink",
    "build:binary": "bun build --compile index.ts --outfile bunim",
    "build:binary:prod": "bun build --compile index.ts --outfile bunim --minify --sourcemap",
    "build:binary:all": "bun build --compile index.ts --outfile bunim --target bun-darwin-x64 && bun build --compile index.ts --outfile bunim-x64 --target bun-linux-x64 && bun build --compile index.ts --outfile bunim-arm64 --target bun-linux-arm64"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "boolean-sat": "^1.0.0",
    "commander": "^14.0.3",
    "simple-git": "^3.30.0"
  }
}